using System;

class Program
{
    static void Main()
    {
        Console.Write("Digite um número: ");
        int numero = int.Parse(Console.ReadLine());

        if (numero != 0)
        {
            Console.WriteLine("O número é diferente de zero");
        }
        else
        {
            Console.WriteLine("O número é igual a zero");
        }
    }
}
