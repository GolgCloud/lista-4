using System;

class Program
{
    static void Main()
    {
        Console.Write("Você não é um robô? (sim/não): ");
        string resposta = Console.ReadLine().ToLower(); // Lê a resposta e converte para minúsculas

        if (resposta == "não" || resposta == "nao")
        {
            Console.WriteLine("Por favor, prove que você não é um robô");
        }
        else
        {
            Console.WriteLine("Obrigado por confirmar que você não é um robô!");
        }
    }
}
