using System;

class Program
{
    static void Main()
    {
        Console.Write("Você não é um membro inativo do clube? (sim/não): ");
        string resposta = Console.ReadLine().ToLower(); 

        if (resposta == "não" || resposta == "nao")
        {
            Console.WriteLine("Por favor, atualize sua inscrição para continuar usufruindo dos benefícios do clube");
        }
        else
        {
            Console.WriteLine("Obrigado por manter sua inscrição ativa no clube!");
        }
    }
}
